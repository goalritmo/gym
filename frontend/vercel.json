{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_SUPABASE_URL": "@gym_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@gym_supabase_anon_key",
    "VITE_API_BASE_URL": "@gym_api_base_url"
  },
  "build": {
    "env": {
      "VITE_SUPABASE_URL": "@gym_supabase_url",
      "VITE_SUPABASE_ANON_KEY": "@gym_supabase_anon_key", 
      "VITE_API_BASE_URL": "@gym_api_base_url"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
